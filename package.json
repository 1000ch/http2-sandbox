    "lint:js": "eslint **/*.js",
    "lint:css": "stylelint **/*.css",
    "start": "node start.js"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "body-parser": "^1.6.4",
    "compression": "^1.5.1",
