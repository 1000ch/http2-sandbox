    "stylelint": "^6.2.2",
    "stylelint-config-standard": "^6.0.0",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.6.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@1000ch/eslint-config",
      "@1000ch/eslint-config/react"
    ]
