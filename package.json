    "csurf": "^1.6.3",
    "debug": "^2.0.0",
    "express": "^4.3.2",
    "express-state": "^1.2.0",
    "fluxible": "^1.0.0",
    "fluxible-addons-react": "^0.2.0",
    "fluxible-plugin-fetchr": "^0.3.0",
    "fluxible-router": "^0.4.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.1.6"
  },
  "devDependencies": {
    "@1000ch/eslint-config": "^1.2.0",
    "babel-eslint": "^6.0.4",
